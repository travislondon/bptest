<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.search.searchPages">
      <page
            canSearchEnclosingProjects="true"
            class="org.xtuml.bp.ui.search.pages.ModelSearchPage"
            enabled="true"
            icon="icons/bp_search.gif"
            id="org.xtuml.bp.ui.search.xtumlSearchPage"
            label="xtuml Search"
            showScopeSection="true"
            tabPosition="0">
      </page>
   </extension>
   <extension
         point="org.eclipse.search.searchResultViewPages">
      <viewPage
            class="org.xtuml.bp.ui.search.pages.ModelSearchResultPage"
            icon="icons/bp_search.gif"
            id="org.xtuml.bp.ui.search.modelSearchResultPage"
            label="Model Search"
            searchResultClass="org.xtuml.bp.search.results.ModelSearchResult">
      </viewPage>
   </extension>

</plugin>
